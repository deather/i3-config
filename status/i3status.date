general {
#	output_format = "dzen2"
	colors = true
	interval = 1
}

order += "cpu_usage"
order += "load"
order += "disk /"
order += "volume master"
order += "time"

time {
	format = " %d-%m-%Y (S%V) -  %H:%M:%S"
}


volume master {
	format = " : %volume"
	device = "default"
	mixer = "Master"
	mixer_idx = 0
}


cpu_usage {
	format = " %usage"
}

load {
	format = "%1min"
}

disk "/" {
	format = " %free"
}